{
  "name": "appt-kennisbank",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "next dev",
    "build:acceptance": "env-cmd -f .env.acceptance next build",
    "build:production": "env-cmd -f .env.production next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start -p ${PORT:=3000}",
    "watch:ts": "tsc --watch",
    "lint:watch": "next lint --watch",
    "lint": "next lint && tsc --noEmit",
    "generate-types": "env-cmd -f .env.development contentful-typescript-codegen --output shared/api/contentful/contentful-types.ts",
    "contentful:export": "env-cmd -f .env.production node scripts/contentful-export.js"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.12.1",
    "@google-cloud/storage": "^5.19.4",
    "@headlessui/react": "^1.6.1",
    "@next/bundle-analyzer": "^12.1.5",
    "@q42/floating-focus-a11y": "^1.3.3",
    "babel-plugin-prismjs": "^2.1.0",
    "classnames": "^2.3.1",
    "contentful": "^9.1.12",
    "contentful-management": "^10.3.0",
    "focus-trap-react": "^8.9.2",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "next": "^12.3.0",
    "next-i18next": "^11.0.0",
    "next-seo": "^5.4.0",
    "next-themes": "0.2.0",
    "node-cache": "^5.1.2",
    "prismjs": "^1.27.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-youtube": "^9.0.3",
    "sharp": "^0.30.2",
    "swr": "^1.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@contentful/rich-text-types": "^15.11.1",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "16.11.9",
    "@types/prismjs": "^1.26.0",
    "@types/react": "17.0.35",
    "@types/uuid": "^8.3.4",
    "@types/youtube-player": "^5.5.6",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "contentful-typescript-codegen": "^3.2.3",
    "env-cmd": "^10.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  }
